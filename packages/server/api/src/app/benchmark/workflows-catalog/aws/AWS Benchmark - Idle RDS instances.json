                                    "sourceCode": {
                                      "code": "export const code = async (inputs) => {\n  const rdsDatabase = (typeof inputs.rdsDatabase === 'string'\n    ? JSON.parse(inputs.rdsDatabase)\n    : inputs.rdsDatabase) as any;\n\n  const deploymentOption = rdsDatabase.MultiAZ ? 'Multi-AZ' : 'Single-AZ';\n\n  const engine = (rdsDatabase.Engine || '').toLowerCase();\n  let databaseEngine = 'Unknown';\n\n  const isOnOutpost = rdsDatabase.DBSubnetGroup?.Subnets?.some(\n    (subnet: any) => subnet.SubnetOutpost && Object.keys(subnet.SubnetOutpost).length > 0\n  ) ?? false;\n\n  if (engine.startsWith('aurora-postgresql')) {\n    databaseEngine = 'Aurora PostgreSQL';\n  } else if (engine.startsWith('aurora-mysql') || engine.startsWith('aurora')) {\n    databaseEngine = 'Aurora MySQL';\n  } else if (engine.includes('db2')) {\n    databaseEngine = 'Db2';\n  } else if (engine.startsWith('mariadb')) {\n    databaseEngine = 'MariaDB';\n  } else if (engine.startsWith('mysql')) {\n    databaseEngine = isOnOutpost ? 'MySQL (on-premise for Outpost)' : 'MySQL';\n  } else if (engine.startsWith('postgres')) {\n    databaseEngine = 'PostgreSQL';\n  } else if (engine.startsWith('sqlserver')) {\n    databaseEngine = isOnOutpost ? 'SQL Server (on-premise for Outpost)' : 'SQL Server';\n  } else if (engine) {\n    databaseEngine = engine.charAt(0).toUpperCase() + engine.slice(1);\n  }\n\n  return {\n    deploymentOption,\n    databaseEngine,\n  };\n};",
                                      "packageJson": "{}"
                                    },
