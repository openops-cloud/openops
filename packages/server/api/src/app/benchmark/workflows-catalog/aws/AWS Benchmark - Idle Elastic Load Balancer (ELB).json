                            },
                            "sourceCode": {
                              "code": "export const code = async ({ arn }) => {\n  return parseElbArn(arn);\n};\n\nconst parseElbArn = (arn) => {\n  if (typeof arn !== 'string') {\n    throw new Error('ARN must be a string');\n  }\n\n  const parts = arn.split(':');\n  if (parts.length < 6 || !parts[5]) {\n    throw new Error('Invalid ARN format');\n  }\n\n  const accountId = parts[4]; // <-- Extract account ID\n\n  const resource = parts[5];\n  const resourceParts = resource.split('/');\n\n  if (resourceParts.length < 2 || resourceParts[0] !== 'loadbalancer') {\n    throw new Error('ARN does not represent a Load Balancer');\n  }\n\n  const lbType = resourceParts[1]; // 'app', 'net', 'gateway', or 'classic'\n  let type;\n\n  if (lbType === 'app') {\n    type = 'Application';\n  } else if (lbType === 'net') {\n    type = 'Network';\n  } else if (lbType === 'gateway') {\n    type = 'Gateway';\n  } else if (lbType === 'classic') {\n    type = 'Classic';\n  } else {\n    throw new Error(`Unknown Load Balancer type: ${lbType}`);\n  }\n\n  const pricingParam = (type === 'Classic') ? 'AWSELB' : `Load Balancer-${type}`;\n  const namespace = (type === 'Classic') ? 'AWS/ELB' : `AWS/${type}ELB`;\n\n  const loadBalancerName = resourceParts.slice(1).join('/');\n\n  return {\n    accountId,\n    namespace,\n    loadBalancerName,\n    pricingParam,\n    type\n  };\n};\n",
                              "packageJson": "{}"
